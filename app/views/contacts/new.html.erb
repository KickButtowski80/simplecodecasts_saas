<div class='row'>
    <%= form_for @contact do |f| %>
    <div class='form-group'>
        <%= f.label :name %>
        <%= f.text_field :name %>
    </div> 
    <div class='form-group'>
        <%= f.lable :email %>
        <%= f.email_field :email %>
    </div>  
    <div class='form-group'>
        <%= f.lable :comments %>
        <%= f.text_area :comments %>
    </div>    
    <% end %>
    
</div>